local function executeCommand(cmd)
    -- Split the command into words
    local words = {}
    for word in cmd:gmatch("%S+") do
        table.insert(words, word)
    end
    
    -- Extract the command name and arguments
    local cmdName = table.remove(words, 1)
    local args = words

    -- Check if cmd exists
    if cmdName == "test" then
        Error.Visible = false
        Success.Visible = true
        Success.Text = "Success: " .. cmdName .. " executed"
        print("Test command executed with args:", table.concat(args, ", "))
    elseif cmdName == "testarg" and args[1] then
        Error.Visible = false
        Success.Visible = true
        Success.Text = "Success: " .. cmdName .. " executed"
        print("Another command executed with args:", table.concat(args, ", "))
    else
        Error.Visible = true
        Success.Visible = false
        Error.Text = "Command not recognized: " .. cmdName
        print("Command not recognized:", cmdName)
    end
end



-- Event handler for input submission
Input.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        local cmd = Input.Text
        Input.Text = ""
        executeCommand(cmd)
    end
end)
